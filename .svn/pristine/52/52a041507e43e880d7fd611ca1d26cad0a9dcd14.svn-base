package com.zplh.zplh_android_yk.bean;

import java.io.Serializable;

/**
 * Created by zhangshuai on 2017/11/6.
 * Description: 通讯录加好友时，把好友信息传到服务器
 */

public class AliFriendsMessageBean implements Serializable {
    private String uid; //设备名称
    private String account;// 当前支付宝帐号
    private String friends_number; //新增好友的手机号
    private String friends_name ;//新增好友的名字

    public String getUid() {
        return uid;
    }

    public void setUid(String uid) {
        this.uid = uid;
    }

    public String getFriends_number() {
        return friends_number;
    }

    public void setFriends_number(String friends_number) {
        this.friends_number = friends_number;
    }

    public String getAccount() {
        return account;
    }

    public void setAccount(String account) {
        this.account = account;
    }

    public String getFriends_name() {
        return friends_name;
    }

    public void setFriends_name(String friends_name) {
        this.friends_name = friends_name;
    }

    public AliFriendsMessageBean(String uid, String account , String  friends_number,String friends_name ) {
        this.uid = uid;
        this.account = account;
        this.friends_number = friends_number;
        this.friends_name=friends_name;

    }

}
